          </material>
        </visual>
      </link>
    </model>

    <!-- Pothole (Terrain Challenge) -->
    <model name="pothole">
      <static>true</static>
      <pose>-3 1 -0.08 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.4</radius>
              <length>0.1</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
