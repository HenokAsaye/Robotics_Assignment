<?xml version="1.0" encoding="UTF-8"?>
<sdf version="1.7">
  <model name="medbot">
    <!-- Base footprint (for fixed base assumption) -->
    <link name="base_footprint">
      <inertial>
        <mass>30.0</mass>
        <inertia>
          <ixx>0.1</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy>
          <iyz>0.0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Main chassis -->
    <link name="base_link">
      <pose relative_to="base_footprint">0.0 0.0 0.085 0 0 0</pose>
      <inertial>
        <mass>28.0</mass>
        <inertia>
          <ixx>0.5</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.5</iyy>
          <iyz>0.0</iyz>
          <izz>0.3</izz>
        </inertia>
      </inertial>
      <visual name="chassis_visual">
        <geometry>
          <box>
            <size>0.4 0.3 0.15</size>
          </box>
        </geometry>
        <material>
          <ambient>0.0 0.8 0.0 1.0</ambient>
          <diffuse>0.0 0.8 0.0 1.0</diffuse>
          <specular>0.1 0.1 0.1 1.0</specular>
        </material>
      </visual>
      <collision name="chassis_collision">
        <geometry>
          <box>
            <size>0.4 0.3 0.15</size>
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Left wheel -->
    <link name="left_wheel_link">
      <pose relative_to="base_link">-0.05 0.16 0.0 1.57 0 0</pose>
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual name="left_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.055</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1.0</ambient>
          <diffuse>0.3 0.3 0.3 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
        </material>
      </visual>
      <collision name="left_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.055</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Right wheel -->
    <link name="right_wheel_link">
      <pose relative_to="base_link">-0.05 -0.16 0.0 1.57 0 0</pose>
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual name="right_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.055</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1.0</ambient>
          <diffuse>0.3 0.3 0.3 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
        </material>
      </visual>
      <collision name="right_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.055</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Caster wheel -->
    <link name="caster_wheel_link">
      <pose relative_to="base_link">0.15 0.0 -0.05 0 0 0</pose>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.001</iyy>
          <iyz>0.0</iyz>
          <izz>0.001</izz>
        </inertia>
      </inertial>
      <visual name="caster_wheel_visual">
        <geometry>
          <sphere>
            <radius>0.025</radius>
          </sphere>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.2 1.0</ambient>
          <diffuse>0.4 0.4 0.4 1.0</diffuse>
          <specular>0.0 0.0 0.0 1.0</specular>
        </material>
      </visual>
      <collision name="caster_wheel_collision">
        <geometry>
          <sphere>
            <radius>0.025</radius>
          </sphere>
        </geometry>
      </collision>
    </link>

    <!-- Lidar -->
    <link name="lidar_link">
      <pose relative_to="base_link">0.0 0.0 0.15 0 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.001</iyy>
          <iyz>0.0</iyz>
          <izz>0.001</izz>
        </inertia>
      </inertial>
      <visual name="lidar_visual">
        <geometry>
          <cylinder>
            <radius>0.04</radius>
            <length>0.02</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.0 1.0</ambient>
          <diffuse>0.1 0.1 0.1 1.0</diffuse>
          <specular>0.5 0.5 0.5 1.0</specular>
        </material>
      </visual>
    </link>

    <!-- Camera -->
    <link name="camera_link">
      <pose relative_to="base_link">0.2 0.0 0.08 0 0 0</pose>
      <inertial>
        <mass>0.05</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.0001</iyy>
          <iyz>0.0</iyz>
          <izz>0.0001</izz>
        </inertia>
      </inertial>
      <visual name="camera_visual">
        <geometry>
          <box>
            <size>0.02 0.04 0.02</size>
          </box>
        </geometry>
        <material>
          <ambient>0.0 0.0 0.5 1.0</ambient>
          <diffuse>0.0 0.0 0.8 1.0</diffuse>
          <specular>0.1 0.1 0.1 1.0</specular>
        </material>
      </visual>
    </link>

    <!-- IMU -->
    <link name="imu_link">
      <pose relative_to="base_link">0.0 0.0 0.07 0 0 0</pose>
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.00001</iyy>
          <iyz>0.0</iyz>
          <izz>0.00001</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Dummy delivery compartment link -->
    <link name="delivery_compartment_link">
      <pose relative_to="base_link">-0.1 0.0 0.0 0 0 0</pose>
      <inertial>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.01</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual name="compartment_visual">
        <geometry>
          <box>
            <size>0.2 0.2 0.1</size>
          </box>
        </geometry>
        <material>
          <ambient>0.8 0.6 0.0 1.0</ambient>
          <diffuse>0.8 0.6 0.0 1.0</diffuse>
          <specular>0.1 0.1 0.1 1.0</specular>
        </material>
      </visual>
    </link>

    <!-- Medical cross (visual only) -->
    <link name="medical_cross_link">
      <pose relative_to="base_link">0.0 0.0 0.15 0 0 0</pose>
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.00001</iyy>
          <iyz>0.0</iyz>
          <izz>0.00001</izz>
        </inertia>
      </inertial>
      <visual name="cross_visual">
        <geometry>
          <box>
            <size>0.05 0.05 0.01</size>
          </box>
        </geometry>
        <material>
          <ambient>1.0 0.0 0.0 1.0</ambient>
          <diffuse>1.0 0.0 0.0 1.0</diffuse>
          <specular>0.1 0.1 0.1 1.0</specular>
        </material>
      </visual>
    </link>

    <!-- Camera optical frame (dummy) -->
    <link name="camera_optical_link">
      <pose relative_to="camera_link">0.0 0.0 0.0 -1.5707963267948966 0 -1.5707963267948966</pose>
      <inertial>
        <mass>0.001</mass>
        <inertia>
          <ixx>0.00001</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.00001</iyy>
          <iyz>0.0</iyz>
          <izz>0.00001</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Joints -->
    <joint name="base_footprint_base_link" type="fixed">
      <parent>base_footprint</parent>
      <child>base_link</child>
    </joint>

    <joint name="left_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>left_wheel_link</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1e16</lower>
          <upper>1e16</upper>
        </limit>
        <dynamics>
          <friction>0.0</friction>
          <damping>0.1</damping>
        </dynamics>
      </axis>
    </joint>

    <joint name="right_wheel_joint" type="revolute">
      <parent>base_link</parent>
      <child>right_wheel_link</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1e16</lower>
          <upper>1e16</upper>
        </limit>
        <dynamics>
          <friction>0.0</friction>
          <damping>0.1</damping>
        </dynamics>
      </axis>
    </joint>

    <joint name="caster_wheel_joint" type="ball">
      <parent>base_link</parent>
      <child>caster_wheel_link</child>
    </joint>

    <joint name="lidar_joint" type="fixed">
      <parent>base_link</parent>
      <child>lidar_link</child>
    </joint>

    <joint name="camera_joint" type="fixed">
      <parent>base_link</parent>
      <child>camera_link</child>
    </joint>

    <joint name="imu_joint" type="fixed">
      <parent>base_link</parent>
      <child>imu_link</child>
    </joint>

    <joint name="delivery_compartment_joint" type="fixed">
      <parent>base_link</parent>
      <child>delivery_compartment_link</child>
    </joint>

    <joint name="medical_cross_joint" type="fixed">
      <parent>base_link</parent>
      <child>medical_cross_link</child>
    </joint>

    <joint name="camera_optical_joint" type="fixed">
      <parent>camera_link</parent>
      <child>camera_optical_link</child>
    </joint>

    <!-- Plugins for Gazebo Harmonic -->
    <plugin filename="gz-sim-diff-drive-system" name="gz::sim::systems::DiffDrive">
      <left_joint>left_wheel_joint</left_joint>
      <right_joint>right_wheel_joint</right_joint>
      <wheel_separation>0.32</wheel_separation>
      <wheel_radius>0.055</wheel_radius>
      <max_linear_acceleration>1.0</max_linear_acceleration>
      <max_angular_acceleration>3.0</max_angular_acceleration>
      <max_linear_velocity>0.5</max_linear_velocity>
      <max_angular_velocity>3.14</max_angular_velocity>
      <topic>/cmd_vel</topic>
      <odom_topic>/odom</odom_topic>
      <odom_frame>odom</odom_frame>
      <robot_base_frame>base_footprint</robot_base_frame>
    </plugin>

    <plugin filename="gz-sim-imu-system" name="gz::sim::systems::Imu">
    </plugin>

  </model>
</sdf>
